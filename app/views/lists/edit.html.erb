<% provide(:title, "Editar lista") %>
<h1>Editar lista</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@list) do |f| %>
      <%#= render 'shared/error_messages' %>

      <%= f.label :name, "Nombre" %>
      <%= f.text_field :name %>

      
      <%= f.submit "Guardar", class: "btn btn-large btn-primary" %>
    <% end %>

        
  </div>
</div>